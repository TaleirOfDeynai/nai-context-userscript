{
  "compilerOptions": {
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "module": "ES2015",
    "paths": {
      "@utils/*": ["../src/utils/*"],
      "@nai/*": ["../src/naiModules/*"],
      "@config": ["../src/config"],
      "@src/*": ["../src/*"],
      "@spec/*": ["./*"]
    },
    "baseUrl": "."
  },
  "exclude": [
      "node_modules"
  ]
}