{
    "name": "nai-context-userscript",
    "version": "0.0.1",
    "description": "Userscript to inject a custom context builder into NovelAI.",
    "keywords": [
        "userscript",
        "greasemonkey",
        "tampermonkey",
        "rollup",
        "typescript",
        "react"
    ],
    "homepage": "https://github.com/TaleirOfDeynai/nai-context-userscript",
    "repository": {
        "type": "git",
        "url": "github:TaleirOfDeynai/nai-context-userscript"
    },
    "bugs": {
        "url": "https://github.com/TaleirOfDeynai/nai-context-userscript/issues"
    },
    "author": "TaleirOfDeynai",
    "license": "UNLICENSE",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "npx rollup --config rollup.config.js",
        "watch": "npx rollup --config rollup.config.js --watch",
        "serve": "node -r esm server.js",
        "start": "node -r esm server.js",
        "build:release": "npx rollup --config rollup.config.js --sourcemap 0 --file dist/release-$npm_package_version.user.js",
        "build:release:win32": "npx rollup --config rollup.config.js --sourcemap 0 --file dist\\release-%npm_package_version%.user.js",
        "version": "npm run build:release && git add dist/release-$npm_package_version.user.js",
        "prepublishOnly": "npm run build:release && git add dist/release-$npm_package_version.user.js"
    },
    "dependencies": {
        "rxjs": "7.5.5",
        "rxjs-for-await": "1.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.15.7",
        "@babel/core": "^7.15.5",
        "@babel/plugin-proposal-class-properties": "^7.14.5",
        "@babel/preset-react": "^7.14.5",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.0",
        "@rollup/plugin-node-resolve": "^13.0.5",
        "@rollup/plugin-replace": "^3.0.0",
        "@rollup/plugin-typescript": "^8.2.5",
        "@types/react": "^18.0.9",
        "colorette": "^2.0.14",
        "esm": "^3.2.25",
        "gpt-3-encoder": "1.1.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "rollup": "^2.58.0",
        "rollup-plugin-userscript-metablock": "^0.3.1",
        "serve": "^12.0.1",
        "ts-node": "10.7.0",
        "tsconfig-paths": "3.14.1",
        "tslib": "^2.3.1",
        "typescript": "latest"
    },
    "config": {
        "port": "8124"
    }
}
